    <body>
        <main>
            <!-- sign up area start -->
            <section
                class="signup__area po-rel-z1 pt-160 pb-145"
                style="position: relative"
            >
                <div
                    class="darklayout"
                    style="
                        position: absolute;
                        top: 0;
                        right: 0;
                        left: 0;
                        background-color: rgba(0, 0, 0, 0.644);
                        z-index: -1;
                        height: 800px;
                    "
                ></div>
                <img
                    src="/images/authbg.jpeg"
                    style="
                        position: absolute;
                        top: 0;
                        right: 0;
                        left: 0;
                        object-fit: cover;
                        z-index: -2;
                        height: 800px;
                    "
                    alt=""
                />
                <div class="container">
                    <div class="row">
                        <div
                            class="col-xxl-6 offset-xxl-3 col-xl-6 offset-xl-3 col-lg-8 offset-lg-2"
                        >
                            <div class="sign__wrapper white-bg">
                                <div
                                    class="sign__header mb-35"
                                    style="padding: 0"
                                >
                                    <h1
                                        style="
                                            text-align: center;
                                            margin-bottom: 20px;
                                        "
                                    >
                                        Thành Công
                                    </h1>
                                    <p style="padding: 0; text-align: center">
                                        Chúng tôi đã gửi tới email của bạn đường
                                        link để thay đổi mật khẩu. Vui lòng kiểm
                                        tra hòm thư.
                                    </p>
                                </div>
                                <div class="sign__form">
                                    <a
                                        href="https://accounts.google.com/signin/v2/identifier?passive=1209600&continue=https%3A%2F%2Faccounts.google.com%2Fb%2F1%2FAddMailService&followup=https%3A%2F%2Faccounts.google.com%2Fb%2F1%2FAddMailService&flowName=GlifWebSignIn&flowEntry=ServiceLogin"
                                        target="_blank"
                                    >
                                        <button
                                            type="button"
                                            class="e-btn w-100"
                                        >
                                            Chuyển tới Email
                                        </button>
                                    </a>
                                    <div class="sign__new text-center mt-20">
                                        <p>
                                            <a
                                                href="/auth/login"
                                                style="color: var(--topicColor)"
                                                >Đăng Nhập</a
                                            >
                                        </p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <!-- sign up area end -->
        </main>
        <script src="assets/js/vendor/jquery-3.5.1.min.js"></script>
